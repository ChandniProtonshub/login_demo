
 <%= form_tag(edit_user_path, :method => "get") do %>
  <p>
    <%= form.label :first_name, "First Name:" %><br>
    <%= form.text_field :first_name %>
  </p>
 
  <p>
    <%= form.label :last_name, "Last Name:" %><br>
    <%= form.text_area :last_name %>
  </p>

  <p>
    <%= form.label :email, "Email id:" %><br>
    <%= form.text_field :email %>
  </p>

  <p>
    <%= form.label :encryted_password, "Password:" %><br>
    <%= form.text_field :encryted_password %>
  </p>
 
  <p>
    <%= form.submit("Create")%>
  </p>
 
<% end %>



