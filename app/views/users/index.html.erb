
<center>
  <h1>Welcome to dashboard </h1>

<p style="text-align: right; font-size: 20px;">
  <%= link_to 'Logout', logout_path %>
</p>

<table>
  <tr>
    <h3><th>First Name</th></h3>
    <th><br></th>
    <th><br></th>
    <h3><th>Last Name</th></h3>
    <th><br></th>
    <th><br></th>
    <h3><th>Email</th></h3>
    <th><br></th>
    <th><br></th>
    <h3><th>Password</th></h3>
    <th><br></th>
    <th><br></th>
    <th colspan="3"></th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.first_name %></td>
      <td><br></td>
      <td><br></td>
      <td><%= user.last_name%></td>
      <td><br></td>
      <td><br></td>
      <td><%= user.email %></td>
      <td><br></td>
      <td><br></td>
      <td><%= user.encrypted_password %></td>
      <td><br></td>
      <td><br></td>
      <td><%= link_to 'Show', user_path(user) %></td>
      <td><br></td>
      <td><br></td>
      <td><%= link_to 'Edit', edit_user_path(user) %></td>
      <td><br></td>
      <td><br></td>
    <% end %>
</table>
<br>
</center>